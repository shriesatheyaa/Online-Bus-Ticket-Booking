<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   
   <class name="model.Route" table="Routes">
       <id name="id" type="int" column="Id" >
       <generator class="assigned"/> 
       </id>

       <many-to-one name="sourceCity" class="model.City" fetch="select">
           <column name="Source_City_Id" not-null="false" />
       </many-to-one>
       
       <many-to-one name="destinationCity" class="model.City" fetch="select">
           <column name="Destination_City_Id" not-null="false" />
       </many-to-one>
       
       <many-to-one name="createdBy" class="model.User" fetch="select">
           <column name="Created_By" not-null="false" />
       </many-to-one>
       <many-to-one name="modifiedBy" class="model.User" fetch="select">
           <column name="Modified_By" not-null="false" />
       </many-to-one>
       <property name="createdAt" column="Created_At" type ="time" not-null="true"/>
       <property name="modifiedAt" column="Modified_At" type="time" not-null="false"/>
      
   </class>
</hibernate-mapping>